{
  "compilerOptions": {
    "target": "es5",
    "module": "esnext",
    "downlevelIteration": true,
    "strict": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "lib": [
        "es6"
    ]
},
  "include": [
    "src", "**/*.ts", "**/*.js"
  ],
  "exclude": [
      "bin/*",
      "bin/**/*"
  ]
}
