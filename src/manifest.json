{
  "$schema": "https://raw.githubusercontent.com/sketch-hq/SketchAPI/develop/docs/sketch-plugin-manifest-schema.json",
  "icon": "icon.png",
  "commands": [
    {
        "name": "无极Sketch UIkit",
        "identifier": "navi.main-identifier",
        "script": "main.ts",
        "shortcut": "ctrl command n",
        "handlers": {
          "run": "onStart",
          "actions": {
            "OpenDocument": "onOpenDocument",
            "SelectionChanged" : "onSelectionChanged",
            "Copy.begin": "beforeCopy",
            "Paste": "onPaste"
          }
        }
    },
    {
        "name": "SVG Insert",
        "identifier": "svgInsert",
        "script": "./svg-insert.js",
        "description": "Insert SVG code as a Layer",
        "icon": "svg-insert.png"
      }
  ],
  "menu": {
    "isRoot": true,
    "title": "Navi",
    "items": [
      "navi.main-identifier",
      "svgInsert"
    ]
  }
}
